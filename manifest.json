{ 
	"name": "Hellodmostagram", 
	"version": "1.0",
	"manifest_version": 2 ,
	"description": "Instalike!", 
	"permissions": [
		"tabs", "webNavigation", "activeTab", "alarms", "webRequest", "*://*/*", "*://*/*"
	], 
	"browser_action": {
		"default_icon": "heart.png",
		"default_title": "Liking!",
		"default_popup": "popup.html"
	},
	"background": {
		"scripts": ["background.js"], 
		"persistent": true
	}, 
	"content_scripts": [ 
		{
		  "matches": [ "https://www.instagram.com/explore/*" ],
		  "js": ["content.js"]
		}]
}