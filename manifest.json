{ 
	"name": "Hellodmostagram", 
	"version": "1.0",
	"manifest_version": 2 ,
	"description": "Instalike!", 
	"permissions": [
		"tabs", "webNavigation", "activeTab", "webRequest", "*://*/*", "*://*/*"
	], 
	"background": {
		"scripts": ["background.js"]
	}, 
	"content_scripts": [ 
		{
		  "matches": [ "https://www.instagram.com/explore/*" ],
		  "js": ["content.js"]
		}]
}